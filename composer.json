{
    "name": "faiare/laravel-notification-channel-exponent",
    "description": "Exponent Push Notifications Driver for Laravel",
    "homepage": "https://github.com/faiare/laravel-notification-channel-exponent",
    "license": "MIT",
    "authors": [
        {
            "name": "faiare",
            "email": "lovelive@fiareapp.red",
            "homepage": "https://github.com/faiare",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/notifications": "^5.5|^5.6|^5.7|^5.8|^6.0|^7.0",
        "illuminate/support": "^5.5|^5.6|^5.7|^5.8|^6.0|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Exponent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Exponent\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Exponent\\ExponentServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
